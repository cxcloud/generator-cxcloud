{
  "swagger": {
    "basePath": "<%= apiPrefix %>/v1",
    "outputDirectory": "./dist",
    "entryFile": "./src/index.ts",
    "name": "<%= projectName %>",
    "description": "<%= projectDescription %>",
    "license": "ISC",
    "produces": ["application/json"],
    "version": "0.0.1",
    "securityDefinitions": {
      "token": {
        "type": "apiKey",
        "name": "Authorization",
        "in": "header"
      }
    }
  }
}
